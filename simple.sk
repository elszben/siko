module Main where

class FEq a

class (Boo a) => Boo a where
   boo a :: a -> Bool

class Quu a where
    quu a :: a -> Bool

instance Boo Int where
    boo a = False

instance Quu Int where
    quu a = False

data Foo a = extern

instance FEq Int
instance FEq Bool
instance (Eq a) => FEq Foo a

foo b = do 
    boo b
    quu b

main = foo 5
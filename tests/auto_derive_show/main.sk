module Main where

import Std.Util

data Color = Red Int | Blue deriving Show
    
data Person = { name :: String, age :: Int} deriving Show
    
main  = do
    color <- "{}" % Red 5
    person <- "{}" % Person { age=5, name = "Lily"}
    assert (color == "Red (5)")
    assert (person == "Person { name: Lily, age: 5 }")
    
    
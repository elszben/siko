module Main where

import Std.Util

plus :: Int -> Int -> Int -> Int -> Int
plus a b c d = a + b + c + d

greeter a b = "{} {}!" % (a, b)

main = do
    a <- plus 123
    a <- a 123
    a <- a 123
    a <- a 123
    assert(a == 123*4)
    g <- greeter "Hello"
    john <- g "John"
    lily <- g "Lily"
    assert(john == "Hello John!")
    assert(lily == "Hello Lily!")

